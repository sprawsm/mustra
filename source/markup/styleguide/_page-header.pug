header.page-header

  .container

    a(href="/").page-header-logo
      img(src="https://i.imgur.com/A0a2nSb.png", width="173", height="40", alt="Mustra")

    button.page-header-toggler.collapsed(type="button" data-toggle="collapse" data-target="#nav" aria-controls="nav" aria-expanded="false" aria-label="Toggle navigation")
      span Menu

    .page-header-collapse.collapse#nav

      ul.menu
        li.menu-item
          a(href="#") Nav link
        li.menu-item
          a(href="#") Nav link
        li.menu-item.dropdown
          a(href="#", role="button" id="menuDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false") Nav link #[span.caret]
          ul.dropdown-menu(aria-labelledby="dropdownMenuLink")
            li
              a.dropdown-item(href="#") Sub-nav link
            li
              a.dropdown-item(href="#") Sub-nav link
            li
              a.dropdown-item(href="#") Sub-nav link
        li.menu-item
          a(href="#") Nav link

      a(href="#").button.button-primary.button-sm.end-sm Button
